{
  "configurations": {
    "Pytest: Launch": {
      "adapter": "debugpy",
      "configuration": {
        "name": "Pytest: Launch",
        "type": "python",
        "request": "launch",
        "python": "/home/dylan/Envs/met/bin/python3.8",
        "console": "integratedTerminal",
        "debugOptions": [],
        "subProcess": true,
        "program": "/home/dylan/Envs/met/bin/pytest",
        "args": [
          "${cwd}/${TestName}"
        ]
      },
      "breakpoints": {
        "exception": {
          "caught": "",
          "uncaught": "",
          "raised": ""
        }
      }
    },


    "mocha": {
      "adapter": "vscode-node",
      "breakpoints": {
        "exceptions": {
          "all": "",
          "uncaught": ""
        }
      },
      "configuration": {
        "request": "launch",
        "name": "Debug Mocha Test",
        "type": "node",
        "runtimeArgs": [
          "--inspect-brk",
          "/home/dylan/.nvm/versions/node/v15.8.0/bin/mocha",
          "${file}",
          "-f",
          "'${TestName}'",
          "--color",
          "--inline-diffs",
          "--full-trace"
        ],
        "console": "integratedTerminal",
        "skipfiles": ["<node_internals>/**/*.js"],
        "internalConsoleOptions": "neverOpen",
        "port": 9229
      }
    },


    "mocha2": {
      "adapter": "vscode-node",
      "breakpoints": {
        "exception": {
          "all": "",
          "uncaught": ""
        }
      },
      "configuration": {
        "args": [
          "-u",
          "bdd",
          "--timeout",
          "999999",
          "--colors",
          "${cwd}/${TestName}"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "openOnSessionStart",
        "name": "Mocha Tests",
        "program": "${cwd}/node_modules/mocha/bin/_mocha",
        "request": "launch",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "envFile": "${cwd}/.env",
        "type": "node"
      }
    },




    "attach": {
      "adapter": "vscode-node",
      "default": true,
      "breakpoints": {
        "exceptions": {
          "all": "N",
          "uncaught": "N"
        }
      },
      "configuration": {
        "name": "Attach to Process",
        "type": "node",
        "request": "attach",
        "skipfiles": ["<node_internals>/**/*.js"],
        "processId": "${processId}"
      }
    }
  }
}
